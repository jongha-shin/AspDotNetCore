@{
    ViewData["Title"] = "시설물 예약";
}
<style>
    .table-hover > tbody > tr {
        cursor: pointer
    }

    table > thead > tr > th {
        background-color: #3763ab;
        color: white;
    }

    .approval_input {
        text-align: center;
        cursor: pointer;
        width: inherit;
        padding: 0px
    }

    .nav-tabs .nav-link.active {
        font-weight: bold;
        color: #214085
    }

    .nav-tabs .nav-link {
        color: grey;
    }

    label {
        font-weight: bold
    }
</style>
@model List<Etc_List>
<div class="text-center col-12 col-md-8 offset-md-2 p-2" style="background-color:white">
    <ul class="nav nav-tabs mb-3" id="pills-tab" role="tablist">
        <li class="nav-item">
            <a class="nav-link" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab" aria-controls="pills-home" aria-selected="true" onclick="showSelectYear('N')">예약 및 현황</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="pills-profile-tab" data-toggle="pill" href="#pills-profile" role="tab" aria-controls="pills-profile" aria-selected="false" onclick="showSelectYear('Y')">예약내역p</a>
        </li>
        <ul class="nav justify-content-end" id="SelectYear" style="font-size:small; position:absolute; right:0px; top: 11px; display:none;">
            <a href="#" id="prevBtn" onclick="changeYear('-')"><img src="~/img/search-prev.png" style="width:13px; height:20px; margin:5px" /></a>
            <span style="margin:5px">@ViewBag.선택년 년</span>
            <a href="#" id="nextBtn" onclick="changeYear('+')"><img src="~/img/search-next.png" style="width:13px; height:20px; margin:5px" /></a>
        </ul>
    </ul>
</div>
<div id="calendar"></div>
